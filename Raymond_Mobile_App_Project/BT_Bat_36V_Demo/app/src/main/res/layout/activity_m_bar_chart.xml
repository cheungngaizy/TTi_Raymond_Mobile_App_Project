<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_bar_background"
    tools:context="com.example.ptencheu.bt_bat_36v_demo.mBarChartAct">


    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/BarChart"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="50dp"
        android:background="@color/color_bar_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_Bar"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="55dp"
        android:layout_height="39dp"
        android:layout_marginRight="521dp"
        android:text="Back"
        android:textSize="10sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textView_Percentage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar_0_10"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="370dp"
        android:layout_marginTop="40dp"
        android:rotation="270"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/BarChart"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar_11_20"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="287dp"
        android:layout_marginTop="40dp"
        android:rotation="270"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/BarChart"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar_21_30"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="205dp"
        android:layout_marginTop="40dp"
        android:rotation="270"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/BarChart"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar_31_40"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="125dp"
        android:layout_marginTop="40dp"
        android:rotation="270"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/BarChart"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar_41_50"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="46dp"
        android:layout_marginTop="40dp"
        android:rotation="270"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/BarChart"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar_51_60"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="385dp"
        android:layout_marginTop="40dp"
        android:rotation="270"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/BarChart"
        app:layout_constraintLeft_toLeftOf="@+id/BarChart"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_TargetSpeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/BarChart" />

    <TextView
        android:id="@+id/Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="-2dp"
        android:gravity="center"
        android:text="Statistics"
        android:textColor="@android:color/background_light"
        android:textSize="50sp"
        app:layout_constraintHorizontal_bias="0.261"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mLeft_cutting_tim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="TextView"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/mRight_Ah"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Title"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/mRight_Ah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintLeft_toRightOf="@+id/Title"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/range"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="38dp"
        android:text="0         5         10        15         20        25        30        35       40        45       50        55        60"
        android:textColor="@android:color/white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="7dp" />

</android.support.constraint.ConstraintLayout>
